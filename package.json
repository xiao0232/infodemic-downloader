    "@material-ui/styles": "^5.0.0-beta.5",
    "axios": "^0.21.1",
    "date-fns": "^2.23.0",
    "moment": "^2.29.1",
    "react": "^17.0.0",
    "react-code-blocks": "^0.0.9-0",
    "react-dom": "^17.0.0"
